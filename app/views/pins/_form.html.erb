<%= form_for @pin do |f| %>
  <%= f.label :disney_num, "Disney Pin ID" %>
  <%= f.text_field :disney_num %>

  <%= f.label :image %>
  <%= f.text_field :image %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_area :description %>
<br></br>
  <%= f.label :series_id, "Select a Series" %>
  <%= f.collection_select :series_id, Series.all, :id, :name %>

  <%= f.submit %>
<% end %>
